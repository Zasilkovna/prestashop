{
	"name": "zasilkovna/prestashop",
	"require": {
		"php": "^7.0 <7.4"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.74",
		"spaze/phpstan-disallowed-calls": "^1.1",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"classmap": [
			"packetery/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse packetery -c phpstan.neon --level 8 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
